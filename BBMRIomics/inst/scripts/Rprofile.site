##do not ask for saving and prevent load .RData/.Rhistory
utils::assignInNamespace("q",
   function(save = "no", status = 0, runLast = TRUE) {
     .Internal(quit(save, status, runLast))
   }, "base")

utils::assignInNamespace("quit",
   function(save = "no", status = 0, runLast = TRUE) {
     .Internal(quit(save, status, runLast))
   }, "base")
